InvishackerColor.BackgroundColor3 = red
Invishacker.MouseButton1Click:Connect(function()
    if InvishackerColor.BackgroundColor3 == red then
        InvishackerColor.BackgroundColor3 = green
                    
        local rsrv = game:GetService("RunService")
        local heartbeat = rsrv.Heartbeat
        local renderstepped = rsrv.RenderStepped
        
        local lp = game.Players.LocalPlayer
        local mouse = lp:GetMouse()
        
        local chr = lp.Character
        local hum = chr.Humanoid
        local root = hum.RootPart
        
        local isinvisible = false
        
        local visible_parts = {}
        
        for _, v in pairs(chr:GetDescendants()) do
            if v:IsA("BasePart") and v.Transparency == 0 then
                visible_parts[#visible_parts + 1] = v
            end
        end
        
        local function ghost_parts()
            for _, v in pairs(visible_parts) do
                v.Transparency = v.Transparency == 0 and 0.5 or 0
            end
        end
        
        local kdown = mouse.KeyDown:Connect(function(key)
            if key == "g" then
                print("sigma")
            end
        end)
        
        local loop = heartbeat:Connect(function()
            if isinvisible then
                local oldcf = root.CFrame
                local oldcamoffset = hum.CameraOffset
                local newcf = oldcf * CFrame.new(math.random(-1000,1000),500,0)
                hum.CameraOffset = newcf:ToObjectSpace(CFrame.new(oldcf.Position)).Position
                if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then
                    root.CFrame = oldcf * CFrame.new(0,1,0)
                else
                root.CFrame = newcf
                end
                renderstepped:Wait()
        
                hum.CameraOffset = oldcamoffset
                root.CFrame = oldcf
            end
        end)
        
        _G.cons = {kdown, loop}
        isinvisible = true
        game.StarterGui:SetCore("SendNotification", {
            Title = "Trun on invis"; 

            Duration = 60;
            })
            wait(0.1)
    else
        InvishackerColor.BackgroundColor3 = red
        if _G.cons then
            for _, v in pairs(_G.cons) do
                v:Disconnect()
            end
            _G.cons = nil
        end
    end
end)

