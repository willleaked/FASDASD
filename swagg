                            local surg = game.Players.LocalPlayer.Backpack:FindFirstChild("Combat")
                            if surg then
                            surg.Parent = game.Players.LocalPlayer.Character
                            end
                            local New = game.Players.LocalPlayer.Character:FindFirstChild("Combat")
                            if New then
                            New:Activate()
                            end
                            if OWNER.Character:FindFirstChild("BodyEffects"):FindFirstChild("Movement"):FindFirstChild("ReduceWalk") and badping == false then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(CharacterEmmy.HumanoidRootPart.Position + Vector3.new(0,-4.8,0) + (CharacterEmmy.Humanoid.MoveDirection*0.40*CharacterEmmy.Humanoid.WalkSpeed))*CFrame.Angles(math.pi*0.5,0,0)
                                OWNER.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                            elseif OWNER.Character.BodyEffects.Grabbed.Value == nil and not OWNER.Character:FindFirstChild("BodyEffects"):FindFirstChild("Movement"):FindFirstChild("ReduceWalk") then
                                OWNER.Character.HumanoidRootPart.CFrame = CFrame.new(math.random(-100,400),500,math.random(-100,400))
                                OWNER.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                            end
                        end
                        if badping and CharacterEmmy:findFirstChild("BodyEffects"):FindFirstChild("K.O").Value == false then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(CharacterEmmy.HumanoidRootPart.Position + Vector3.new(0,-4.8,0) + (CharacterEmmy.Humanoid.MoveDirection*0.40*CharacterEmmy.Humanoid.WalkSpeed))*CFrame.Angles(math.pi*0.5,0,0)
                        end
                    end
                end
            end)
        end;
        local Start = function()    
            OldFlingPos = player.Character.HumanoidRootPart.Position
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        end;
        Start()	
        repeat wait() until Bringcombat.Text == 'Bring' or OWNER.Character.BodyEffects.Grabbed.Value ~= nil
        Pause()
        Bringcombat.Text = 'Bring'
    else
        Bringcombat.Text = 'Bring'
    end
end)
Gunkill.MouseButton1Click:Connect(function()
    if Gunkill.Text == 'Gunkill' then
        Gunkill.Text = 'un Gunkill'
        if game.Players.LocalPlayer.Backpack:findFirstChild("[Flamethrower]") or game.Players.LocalPlayer.Character:findFirstChild("[Flamethrower]") then
            buying = false
            elseif not game.Players.LocalPlayer.Backpack:findFirstChild("[Flamethrower]") or not game.Players.LocalPlayer.Character:findFirstChild("[Flamethrower]") or game:GetService("Players").LocalPlayer.Backpack["[Flamethrower]"].Ammo.Value <= 0 then
            buying = true
            local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
            local Brokie = workspace.Ignored.Shop["[Flamethrower] - $15914"]
            local BROIMMO = workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"]
            game.Players.LocalPlayer.Character:MoveTo(Brokie.Head.Position) 
            wait(0.25)
            fireclickdetector(Brokie.ClickDetector)
            wait(0.25)
            game.Players.LocalPlayer.Character:MoveTo(BROIMMO.Head.Position)
            fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector) wait(1)
            fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector) wait(1)
            fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector) wait(1)
            fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector) wait(1)
            fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector) wait(1)
            fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector) wait(1)
            fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector)
            wait(.1)
            game.Players.LocalPlayer.character.HumanoidRootPart.CFrame = CFrame.new(savepos)
            buying = false
            end
            repeat task.wait() until buying == false
        local Loop
        AnimPlay(2807049953)
        local OldFlingPos = player.Character.HumanoidRootPart.Position
        local delta = 0
        OWNER.Backpack:FindFirstChild("[Flamethrower]").GripPos = Vector3.new(0,-10,-10)
        local duration = 0.92
    _G.VW = game:GetService("RunService").Stepped:Connect(function(a,b)delta = (delta + b/duration) % 1 end)
        local loopFunction = function()
            local success,err = pcall(function()
                local CharacterEmmy = Players:FindFirstChild(TargetTextbox.Text).Character
                local OldY = CharacterEmmy.HumanoidRootPart.Position.Y+0

                if CharacterEmmy and player.Character then
                    FlingTorso = CharacterEmmy.UpperTorso
                    local dis = 3.85
                    local increase = 6
                    if CharacterEmmy.Humanoid.MoveDirection.X < 0 then
                        xchange = -increase
                    elseif CharacterEmmy.Humanoid.MoveDirection.X > 0  then
                        xchange = increase
                    else
                        xchange = 0
                    end
                    if CharacterEmmy.Humanoid.MoveDirection.Z < 0 then 
                        zchange = -increase
                    elseif CharacterEmmy.Humanoid.MoveDirection.Z > 0 then
                        zchange = increase 
                    else
                        zchange = 0
                    end          
                    if player.Character then
                        if CharacterEmmy:findFirstChild("BodyEffects"):FindFirstChild("K.O").Value == true then
                            local New = game.Players.LocalPlayer.Character:FindFirstChild("[Flamethrower]")
                            if New then
                             New:Deactivate()
                            end 
                            if OWNER.Character:FindFirstChild("BodyEffects"):findFirstChild("Attacking").Value == false then
                            if CharacterEmmy:findFirstChild("BodyEffects"):FindFirstChild("Dead").Value == false then
                             OWNER.Character.HumanoidRootPart.CFrame = CFrame.new(CharacterEmmy.UpperTorso.Position) * CFrame.new(0,3,0)
                             game.ReplicatedStorage.MainEvent:FireServer("Stomp")
                            else
                                OWNER.Character.HumanoidRootPart.CFrame = CFrame.new(math.random(-100,400),500,math.random(-100,400))
                            end
                            else
                                OWNER.Character.HumanoidRootPart.CFrame = CFrame.new(math.random(-100,400),500,math.random(-100,400))
                            end
                        elseif CharacterEmmy:findFirstChild("BodyEffects"):FindFirstChild("K.O").Value == false and OWNER.Character.BodyEffects.Grabbed.Value == nil then
                            local surg = game.Players.LocalPlayer.Backpack:FindFirstChild("[Flamethrower]")
                            if surg then
                            surg.Parent = game.Players.LocalPlayer.Character
                            end
                            local New = game.Players.LocalPlayer.Character:FindFirstChild("[Flamethrower]")
                            if New then
                            New:Activate()
                            end
                            OWNER.Character.HumanoidRootPart.CFrame = CFrame.new(CharacterEmmy.HumanoidRootPart.Position + Vector3.new(0,-11,0) + (CharacterEmmy.Humanoid.MoveDirection*.4*CharacterEmmy.Humanoid.WalkSpeed))*CFrame.Angles(0,5*math.pi*delta,0)*CFrame.new(0,0,20)
                            OWNER.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                        end
                    end
                end
            end)
        end;
        local Start = function()    
            OldFlingPos = player.Character.HumanoidRootPart.Position
            game:GetService("RunService").Stepped:Connect(function(a,b)delta = (delta + b/duration) % 1 end)
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        end;
        Start()	
        repeat wait() until Gunkill.Text == 'Gunkill'
        Pause()
        local New = game.Players.LocalPlayer.Character:FindFirstChild("[Flamethrower]")
        if New then
        New:Deactivate()
        end
        AnimStop(2807049953)
        Gunkill.Text = 'Gunkill'
        game.Players.LocalPlayer.Character:FindFirstChild("[Flamethrower]").Parent = game.Players.LocalPlayer.Backpack
        task.wait()
        game.Players.LocalPlayer.Backpack:FindFirstChild("[Flamethrower]").GripPos = Vector3.new(-0.065873161, -0.288790941, -0.421611279, 0.891013622, 0.449476242, 0.0637640506, -0.453502178, 0.887678444, 0.0797668099, -0.0207486935, -0.0999904573, 0.994772017)
    else
        AnimStop(2807049953)
        Gunkill.Text = 'Gunkill'
        game.Players.LocalPlayer.Character:FindFirstChild("[Flamethrower]").Parent = game.Players.LocalPlayer.Backpack
        task.wait()
        game.Players.LocalPlayer.Backpack:FindFirstChild("[Flamethrower]").GripPos = Vector3.new(-0.065873161, -0.288790941, -0.421611279, 0.891013622, 0.449476242, 0.0637640506, -0.453502178, 0.887678444, 0.0797668099, -0.0207486935, -0.0999904573, 0.994772017)
    end
end)
