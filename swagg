AntiEffectsColor.BackgroundColor3 = red
AntiEffects.MouseButton1Click:Connect(function()
    if AntiEffectsColor.BackgroundColor3 == red then
        AntiEffectsColor.BackgroundColor3 = green
        if _G.FAKELAG then
            _G.FAKELAG:Disconnect()
        end
        _G.FAKELAG = game:GetService("RunService").Heartbeat:Connect(function()
                local currentvel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(math.random(0,0), math.random(-100,-50), math.random(0,0))

                game:GetService("RunService").RenderStepped:Wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = currentvel
        end)
        game:GetService('RunService'):BindToRenderStep("fake", 0 , function()
        setfflag("S2PhysicsSenderRate",1)
        end)
    else
        AntiEffectsColor.BackgroundColor3 = red
        game:GetService('RunService'):UnbindFromRenderStep("fake")
        setfflag("S2PhysicsSenderRate",15)
        if _G.FAKELAG then
            _G.FAKELAG:Disconnect()
        end
    end
end)

