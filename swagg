AntiRpgColor.BackgroundColor3 = red
AntiRpg.MouseButton1Click:Connect(function()
    if AntiRpgColor.BackgroundColor3 == red then
        AntiRpgColor.BackgroundColor3 = green

        function ANtiRpg()
            local rsrv = game:GetService("RunService")
            local heartbeat = rsrv.Heartbeat
            local renderstepped = rsrv.RenderStepped
            
            local lp = game.Players.LocalPlayer
            local mouse = lp:GetMouse()
            
            local chr = lp.Character
            local hum = chr.Humanoid
            local root = hum.RootPart
            
            
            local visible_parts = {}
            
            for _, v in pairs(chr:GetDescendants()) do
                if v:IsA("BasePart") and v.Transparency == 0 then
                    visible_parts[#visible_parts + 1] = v
                end
            end
            print("New1")
            OWNER = game.Players.localPlayer
            antipart = false
            local LocalPlayer = game.Players.LocalPlayer
            local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
            if _G.LOOPANTIHEAL then _G.LOOPANTIHEAL:Disconnect() end
            _G.LOOPANTIHEAL = heartbeat:Connect(function()
                if not isinvisible then
                for i, v in pairs(workspace.Ignored:GetChildren()) do
                    if v.Name == "Handle" and (v.Position - OWNER.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                        local oldcf = root.CFrame
                        local oldcamoffset = hum.CameraOffset
            
                        local newcf = oldcf * CFrame.new(0,-150,0)
            
                        hum.CameraOffset = newcf:ToObjectSpace(CFrame.new(oldcf.Position)).Position
                        if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then
                            root.CFrame = oldcf * CFrame.new(0,1,0)
                        else
                        root.CFrame = newcf
                        end
                        renderstepped:Wait()
            
                        hum.CameraOffset = oldcamoffset
                        root.CFrame = oldcf
                    elseif v.Name == "Part" and (v.Position - OWNER.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                        local oldcf = root.CFrame
                        local oldcamoffset = hum.CameraOffset
            
                        local newcf = oldcf * CFrame.new(0,-150,0)
            
                        hum.CameraOffset = newcf:ToObjectSpace(CFrame.new(oldcf.Position)).Position
                        if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then
                            root.CFrame = oldcf * CFrame.new(0,1,0)
                        else
                        root.CFrame = newcf
                        end
                        renderstepped:Wait()
            
                        hum.CameraOffset = oldcamoffset
                        root.CFrame = oldcf
            
                    end
                end
            end
            end)
            local LocalPlayer = game.Players.LocalPlayer
            local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
            if _G.LOOPANTIHEARPGL then _G.LOOPANTIHEARPGL:Disconnect() end
            _G.LOOPANTIHEARPGL = heartbeat:Connect(function()
                if not isinvisible then
                    if workspace.Ignored:FindFirstChild("Model") then
                    for i, v in pairs(workspace.Ignored.Model:GetChildren()) do
                        if v.Name == "Launcher" and (v.Position - OWNER.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                            local oldcf = root.CFrame
                            local oldcamoffset = hum.CameraOffset
                
                            local newcf = oldcf * CFrame.new(0,-150,0)
                
                            hum.CameraOffset = newcf:ToObjectSpace(CFrame.new(oldcf.Position)).Position
                            if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then
                                root.CFrame = oldcf * CFrame.new(0,1,0)
                            else
                            root.CFrame = newcf
                            end
                            renderstepped:Wait()
                
                            hum.CameraOffset = oldcamoffset
                            root.CFrame = oldcf
                        end
                    end
                    end
                end
            end)
            end
            
            
            ANtiRpg()
            if getgenv().ANTIRPG then getgenv().ANTIRPG:Disconnect() end
            getgenv().ANTIRPG =  game.Players.LocalPlayer.CharacterAdded:connect(function()
                game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                task.wait(1)
                ANtiRpg()
            end)
    else
        AntiRpgColor.BackgroundColor3 = red
        if getgenv().ANTIRPG then getgenv().ANTIRPG:Disconnect() end
        if _G.LOOPANTIHEAL then _G.LOOPANTIHEAL:Disconnect() end
    end
end)

AutoDropColor.BackgroundColor3 = red
AutoDrop.MouseButton1Click:Connect(function()
    if AutoDropColor.BackgroundColor3 == red then
        AutoDropColor.BackgroundColor3 = green
        local Loop
        local loopFunction = function()
            local DropAm = 10000
            if player.DataFolder.Currency.Value > 10000 then
                DropAm = '10000'
            else
                DropAm = tostring(player.DataFolder.Currency.Value)
            end
            local args = {
                'DropMoney',
                DropAm
            }
            MainEvent:FireServer(unpack(args))
        end;
        local Start = function()
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
        end;
        Start()	
        repeat wait() until AutoDropColor.BackgroundColor3 == red
        Pause()
    else
        AutoDropColor.BackgroundColor3 = red
    end
end)

