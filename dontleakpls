        function Wheel(CFRAME)
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].RBWheel.CFrame = CFRAME
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].RTWheel.CFrame = CFRAME
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].LTWheel.CFrame = CFRAME
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].LBWheel.CFrame = CFRAME
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].RBWheel.Velocity = Vector3.new(0,0,0)
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].RTWheel.Velocity = Vector3.new(0,0,0)
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].LTWheel.Velocity = Vector3.new(0,0,0)
            workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].LBWheel.Velocity = Vector3.new(0,0,0)
        end
        
        CreateTargetAbility("sit!",function()
            if TARGET then
                Stop()
                if not workspace.OldVehicles:FindFirstChild(STAND.Name.."BIKE") then
                    --Chat("Debug: buy car")
                    repeat rs.heartbeat:wait()
                        getRoot(STAND).CFrame = CFrame.new(-897, 21, -611)
                        fireclickdetector(workspace.Ignored.Shop["[FoodsCart] - $16"].ClickDetector)
                    until workspace.OldVehicles:FindFirstChild(STAND.Name.."BIKE") or follow == true
                end
                if not getHumanoid(STAND).Sit == true then
                    --Chat("Debug: Sitting alt")
                    repeat task.wait()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].VehicleSeat.Position+Vector3.new(0,0,-1))
                        wait(.1)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].VehicleSeat.Position+Vector3.new(0,0,-1))
                    until getHumanoid(STAND).Sit == true or follow == true
                end
                --Chat("Debug: Bringing: "..TARGET.Name)
                noclip = true
                repeat rs.heartbeat:wait()
                    Wheel(getRoot(TARGET).CFrame)
                    workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.CFrame = CFrame.new(getRoot(TARGET).Position + Vector3.new(0,0,0) + (getHumanoid(TARGET).MoveDirection*0.85*getHumanoid(TARGET).WalkSpeed))
                    workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.Velocity = Vector3.new(0,0,0)
                until workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat:FindFirstChild("SeatWeld") or KO(TARGET).Value == true or Workspace.Vehicles:FindFirstChild(TARGET.Name) or follow == true
                repeat rs.heartbeat:wait()
                    Wheel(getRoot(OWNER).CFrame*CFrame.new(0,10,10))
                    workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.CFrame = getRoot(OWNER).CFrame*CFrame.new(0,0,-10)
                    workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.Velocity = Vector3.new(0,0,0)
                until not workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat:FindFirstChild("SeatWeld") or follow == true
                workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.CFrame = CFrame.new(0,15,213)
                Wheel(CFrame.new(0,15,213))
            end
        end)
        
        CreateTargetAbility("cart!",function()
            if TARGET then
                Stop()
                task.wait()
                if not workspace.OldVehicles:FindFirstChild(STAND.Name.."BIKE") then
                    --Chat("Debug: buy car")
                    repeat rs.heartbeat:wait()
                        getRoot(STAND).CFrame = CFrame.new(-897, 21, -611)
                        fireclickdetector(workspace.Ignored.Shop["[FoodsCart] - $16"].ClickDetector)
                    until workspace.OldVehicles:FindFirstChild(STAND.Name.."BIKE")
                end
                if not getHumanoid(STAND).Sit == true then
                    --Chat("Debug: Sitting alt")
