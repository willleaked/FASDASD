                    STAND.Character:FindFirstChild('[Flamethrower]'):Activate()
                elseif augmode and not flamemode then
                    gunchecking()
                    getRoot(STAND).CFrame = CFrame.new(getRoot(TARGET).Position + Vector3.new(0,0,0) + (getHumanoid(TARGET).MoveDirection*Actual_Prediction*getHumanoid(TARGET).WalkSpeed))*CFrame.Angles(0,20*math.pi*delta,0)*CFrame.new(0,0,8)            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(math.random(0,0),math.random(0,0), math.random(0,0))
                    STAND.Character:FindFirstChild('[AUG]'):Activate()
