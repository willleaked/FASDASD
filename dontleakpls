        if hohoh == true then
                    TARGET.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)    
                    TARGET.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0,0,0)   
        end
        end)
    
