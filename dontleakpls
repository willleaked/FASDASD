                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].VehicleSeat.Position+Vector3.new(0,0,-1))
                        wait(0.1)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].VehicleSeat.Position+Vector3.new(0,0,-1))
                    until getHumanoid(STAND).Sit == true
                end
                --Chat("Debug: Bringing: "..TARGET.Name)
                noclip = true
                getRoot(STAND).CFrame = getRoot(TARGET).CFrame*CFrame.new(0,10,0)
                wait(.1)
                repeat rs.heartbeat:wait()
                    local Predictionnn = 0.6
        
                    if TARGET.Character.Humanoid.MoveDirection.Magnitude == 0 then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = TARGET.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,4)
                        workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.CFrame = CFrame.new(getRoot(TARGET).Position + Vector3.new(0,0,4) + (getHumanoid(TARGET).MoveDirection*0.85*getHumanoid(TARGET).WalkSpeed))
        
                    else
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(TARGET.Character.HumanoidRootPart.Position.X+(TARGET.Character.HumanoidRootPart.Velocity.X*Predictionnn), TARGET.Character.HumanoidRootPart.Position.Y+0, TARGET.Character.HumanoidRootPart.Position.Z+(TARGET.Character.HumanoidRootPart.Velocity.Z*Predictionnn))*CFrame.new(0,0,2)
                        workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.CFrame = CFrame.new(getRoot(TARGET).Position + Vector3.new(0,0,0) + (getHumanoid(TARGET).MoveDirection*0.85*getHumanoid(TARGET).WalkSpeed))
                    end
                until getHumanoid(TARGET).Sit == true
                repeat rs.heartbeat:wait()
                    Wheel(getRoot(OWNER).CFrame*CFrame.new(0,5,0))
                    workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.CFrame = getRoot(OWNER).CFrame*CFrame.new(0,10,10)
                    getRoot(STAND).CFrame = getRoot(OWNER).CFrame*CFrame.new(0,10,0)
                until getHumanoid(TARGET).Sit == false
                workspace.OldVehicles[game.Players.LocalPlayer.Name.."BIKE"].Seat.CFrame = CFrame.new(0,15,213)
                Wheel(CFrame.new(0,100,0))
                getRoot(STAND).CFrame = CFrame.new(0,100,0)
            end
        end)
        
        
        
