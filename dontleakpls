        end
        
    
        function BuyFlame()
            Stop()
            repeat rs.Stepped:Wait()
                getRoot(STAND).CFrame=game.Workspace.Ignored.Shop["[Flamethrower] - $15914"].Head.CFrame
                fireclickdetector(game.Workspace.Ignored.Shop["[Flamethrower] - $15914"].ClickDetector)
            until STAND.Backpack:FindFirstChild("[Flamethrower]")
            repeat rs.Stepped:Wait()
                fireclickdetector(workspace.Ignored.Shop["140 [Flamethrower Ammo] - $1644"].ClickDetector)
                getRoot(STAND).CFrame=game.Workspace.Ignored.Shop["[Flamethrower] - $15914"].Head.CFrame
            until follow == true
        end
