            Stop()
            TWEEN(CFrame.new(-1044, 28, -67))
            wait(.1)
            for i,v in pairs(STAND.Backpack:GetChildren()) do
                wait(0.05)
                spawn(function()
                    if v.Name == "[Grenade]" then
                        v.Parent = STAND.Character; v:Activate()
                        wait(1)
                        v.Parent = STAND.Character; v:Activate()
                    end
                end)
            end
            wait(2)
            blocking = false
            Bind = false
            noclip = true
            game.ReplicatedStorage.MainEvent:FireServer("LeaveCrew")
