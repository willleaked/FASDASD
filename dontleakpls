        function battool()
            if StopHoverAnim == false then
                getHumanoid(STAND):UnequipTools()
                repeat Wait()
                    getRoot(STAND).CFrame=game.Workspace.Ignored.Shop["[Bat] - $265"].Head.CFrame*CFrame.new(0,-5,0)
                    fireclickdetector(game.Workspace.Ignored.Shop["[Bat] - $265"].ClickDetector)
                until STAND.Backpack:FindFirstChild("[Bat]")
                STAND.Backpack:FindFirstChild("[Bat]").Parent = STAND.Character
                getHumanoid(STAND):UnequipTools()
            end
            game.Players.LocalPlayer.Backpack:FindFirstChild("[Bat]").GripPos = Vector3.new(-1.5,-1,1.55)
            game.Players.LocalPlayer.Backpack:FindFirstChild("[Bat]").GripForward = Vector3.new(0, 0, 0)
            game.Players.LocalPlayer.Backpack:FindFirstChild("[Bat]").GripRight = Vector3.new(0.19607843458652496, 0.019607843831181526, 0.9803922176361084)
            game.Players.LocalPlayer.Backpack:FindFirstChild("[Bat]").GripUp = Vector3.new(-0.9755268096923828, -0.09755268692970276, 0.19705550372600555)
            game.Players.LocalPlayer.Backpack["[Bat]"].Parent = game.Players.LocalPlayer.Character
        end
        
        function stopsing()
            if StopHoverAnim == false then
                getHumanoid(STAND):UnequipTools()
                if not game.Players.LocalPlayer.Backpack:FindFirstChild("[StopSign]") then
                repeat Wait()
                    getRoot(STAND).CFrame=game.Workspace.Ignored.Shop["[StopSign] - $318"].Head.CFrame*CFrame.new(0,-5,0)
                    fireclickdetector(game.Workspace.Ignored.Shop["[StopSign] - $318"].ClickDetector)
                until STAND.Backpack:FindFirstChild("[StopSign]")
                STAND.Backpack:FindFirstChild("[StopSign]").Parent = STAND.Character
                getHumanoid(STAND):UnequipTools()
                game.Players.LocalPlayer.Backpack:FindFirstChild("[StopSign]").GripPos = Vector3.new(-3,-15,0)
            end
            end
        end
        function foranti()
            if StopHoverAnim == false then
                getHumanoid(STAND):UnequipTools()
                if not game.Players.LocalPlayer.Backpack:FindFirstChild("[Knife]") then
                repeat Wait()
                    getRoot(STAND).CFrame=game.workspace.Ignored.Shop["[Knife] - $159"].Head.CFrame*CFrame.new(0,-5,0)
                    fireclickdetector(game.workspace.Ignored.Shop["[Knife] - $159"].ClickDetector)
                until STAND.Backpack:FindFirstChild("[Knife]")
                STAND.Backpack:FindFirstChild("[Knife]").Parent = STAND.Character
                STAND.Character:FindFirstChild('[Knife]'):Activate()
                getHumanoid(STAND):UnequipTools()
            end
            end
        end
        function unkeyowner()
            if StopHoverAnim == false then
                getHumanoid(STAND):UnequipTools()
                repeat Wait()
                    getRoot(STAND).CFrame=game.workspace.Ignored.Shop["[Key] - $133"].Head.CFrame*CFrame.new(0,-5,0)
                    fireclickdetector(game.workspace.Ignored.Shop["[Key] - $133"].ClickDetector)
                until STAND.Backpack:FindFirstChild("[Key]")
                STAND.Backpack:FindFirstChild("[Key]").Parent = STAND.Character
                STAND.Character:FindFirstChild('[Key]'):Activate()
                getHumanoid(STAND):UnequipTools()
                wait(0.1)
                getRoot(STAND).CFrame = CFrame.new(getTorso(OWNER).Position)*CFrame.new(0,0,-1)
                STAND.Backpack:FindFirstChild("[Key]").Parent = STAND.Character
                wait(1)
                STAND.Character:FindFirstChild('[Key]'):Activate()
        
            end
        end
        
        function unkeyTARGET()
            if StopHoverAnim == false then
                getHumanoid(STAND):UnequipTools()
                repeat Wait()
                    getRoot(STAND).CFrame=game.workspace.Ignored.Shop["[Key] - $133"].Head.CFrame*CFrame.new(0,-5,0)
                    fireclickdetector(game.workspace.Ignored.Shop["[Key] - $133"].ClickDetector)
                until STAND.Backpack:FindFirstChild("[Key]")
                STAND.Backpack:FindFirstChild("[Key]").Parent = STAND.Character
                STAND.Character:FindFirstChild('[Key]'):Activate()
                getHumanoid(STAND):UnequipTools()
                wait(0.1)
                getRoot(STAND).CFrame = CFrame.new(getTorso(TARGET).Position)*CFrame.new(0,0,-1)
                STAND.Backpack:FindFirstChild("[Key]").Parent = STAND.Character
                wait(1)
                STAND.Character:FindFirstChild('[Key]'):Activate()
        
            end
        end
        
