-- Table to keep track of all buttons
local buttons = {}

-- Function to create a button with an icon
local function createButton(name, position, text, iconId, clickAction)
    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(0, 200, 0, 50)
    Frame.Position = position
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.BorderSizePixel = 0
    Frame.Parent = ScreenGui

    local Button = Instance.new("TextButton")
    Button.Name = name
    Button.Size = UDim2.new(1, -50, 1, 0)
    Button.Position = UDim2.new(0, 50, 0, 0)
    Button.Text = text
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.BackgroundColor3 = Color3.fromRGB(0, 122, 204)
    Button.BorderSizePixel = 0
    Button.Font = Enum.Font.GothamBold
    Button.TextSize = 20
    Button.Parent = Frame

    -- Add hover effect
    Button.MouseEnter:Connect(function()
        Button.BackgroundColor3 = Color3.fromRGB(0, 153, 255)
    end)

    Button.MouseLeave:Connect(function()
        Button.BackgroundColor3 = Color3.fromRGB(0, 122, 204)
    end)

    -- Button click action
    Button.MouseButton1Click:Connect(function()
        clickAction()
            game:GetService("Players").LocalPlayer.PlayerGui["Animation ui"]:Destroy()
    end)

    local Icon = Instance.new("ImageLabel")
    Icon.Size = UDim2.new(0, 50, 0, 50)
    Icon.Position = UDim2.new(0, 0, 0, 0)
    Icon.Image = "http://www.roblox.com/asset/?id="..iconId
    Icon.BackgroundTransparency = 1
    Icon.Parent = Frame

    table.insert(buttons, Frame)
end

-- Function to handle animations
local function ANIMATION(idle, idle2, walk, run, jump, climb, fall)
    local function onCharacterAdded(character)
        wait(1)
        
        local humanoid = character:WaitForChild("Humanoid")
        
        -- Stop all currently playing animations
        for _, v in ipairs(humanoid:GetPlayingAnimationTracks()) do
            v:Stop()
        end
        
        local animate = character:WaitForChild("Animate")
        
        -- Function to set the animation ID
        local function setAnimation(animation, animationId)
            if typeof(animation) == "Instance" and animation:IsA("Animation") then
                animation.AnimationId = animationId
            else
                warn("Failed to set animation:", animationId)
            end
        end
        
        -- Set animations using provided asset IDs
        setAnimation(animate.idle.Animation1, "http://www.roblox.com/asset/?id="..idle)
        setAnimation(animate.idle.Animation2, "http://www.roblox.com/asset/?id="..idle2)
        setAnimation(animate.walk.WalkAnim, "http://www.roblox.com/asset/?id="..walk)
        setAnimation(animate.run.RunAnim, "http://www.roblox.com/asset/?id="..run)
        setAnimation(animate.jump.JumpAnim, "http://www.roblox.com/asset/?id="..jump)
        setAnimation(animate.climb.ClimbAnim, "http://www.roblox.com/asset/?id="..climb)
        setAnimation(animate.fall.FallAnim, "http://www.roblox.com/asset/?id="..fall)
        task.wait(1)
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 100, 0)
    end

    -- Connect the function to CharacterAdded event
    game.Players.LocalPlayer.CharacterAdded:Connect(onCharacterAdded)

    -- Apply animations to the existing character if present
    if game.Players.LocalPlayer.Character then
        onCharacterAdded(game.Players.LocalPlayer.Character)
    end
    task.wait()
    game.Players.LocalPlayer.Character.Humanoid.Jump = true
end

