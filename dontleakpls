        function Gunmodenana()
            if flamemode then
                if STAND.Backpack:FindFirstChild("[Flamethrower]") then
                    STAND.Backpack:FindFirstChild("[Flamethrower]").GripPos = Vector3.new(0,-10,-10)
                    STAND.Backpack:FindFirstChild("[Flamethrower]").Parent = STAND.Character
                    AnimPlay(2807049953)
                elseif STAND.Character:FindFirstChild("[Flamethrower]") then
                    STAND.Character:FindFirstChild("[Flamethrower]").Parent = STAND.Backpack
                    task.wait(.1)
                    STAND.Backpack:FindFirstChild("[Flamethrower]").GripPos = Vector3.new(0,-10,-10)
                    STAND.Backpack:FindFirstChild("[Flamethrower]").Parent = STAND.Character
                    AnimPlay(2807049953)
                else
                    BuyFlame()
                end
            elseif augmode and not flamemode then
                if STAND.Backpack:FindFirstChild("[AUG]") then
