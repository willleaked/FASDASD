        Create('Lettuce!',function()
            local Character = game.Players.LocalPlayer.Character
            local Backpack = game.Players.LocalPlayer.Backpack
            function ZeroVelocity___(PATH)
                PATH.Velocity = Vector3.zero
                PATH.AssemblyAngularVelocity = Vector3.zero
                PATH.AssemblyLinearVelocity = Vector3.zero
            end
            game:GetService("RunService"):UnbindFromRenderStep("Invisible")
            game:GetService("RunService"):BindToRenderStep("Invisible", -1, function()
                ZeroVelocity___(Character:FindFirstChild("HumanoidRootPart"))
                if not Backpack:FindFirstChild("[Lettuce]") and not Character:FindFirstChild("[Lettuce]") then
                    Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop["[Lettuce] - $5"].Head.CFrame * CFrame.new(0, 5, 0)   
                    fireclickdetector(game.Workspace.Ignored.Shop["[Lettuce] - $5"].ClickDetector)
                elseif Backpack:FindFirstChild("[Lettuce]") then
                    Backpack:FindFirstChild("[Lettuce]").Parent = Character
                elseif Character:FindFirstChild("[Lettuce]") then
                    Character:FindFirstChild("[Lettuce]"):Activate()
                end
            end)   
            repeat task.wait() until follow == true     
        end)
        Create('Dcash!',function()
