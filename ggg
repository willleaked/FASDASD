    local trun = serv:Channel("ฟังชั่นเปิด/ปิด")
    trun:Toggle(
        "anti stomp(ออโต้รีตัว)",
        false,
        function(stomp)
            if stomp == true then
                game:GetService('RunService'):BindToRenderStep("anti-stomp", 0 , function()
                if    game.Players.LocalPlayer.Character.BodyEffects["K.O"].Value == true then
                    game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Dead)
                end
                end)
            elseif stomp == false then
                game:GetService('RunService'):UnbindFromRenderStep("anti-stomp")
            end
        end
    )
    trun:Toggle("กันล็อก(เเบบ เเล็กปลอม โดนยิงยากมาก) solaraไม่ซัพพอต", false, function(ffake)
        if ffake == true then
                if _G.FAKELAG then
                    _G.FAKELAG:Disconnect()
                end
                _G.FAKELAG = game:GetService("RunService").Heartbeat:Connect(function()
                        local currentvel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(math.random(0,0), math.random(-100,-50), math.random(0,0))
    
                        game:GetService("RunService").RenderStepped:Wait()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = currentvel
                end)
                game:GetService('RunService'):BindToRenderStep("fake", 0 , function()
                setfflag("S2PhysicsSenderRate",1)
                end)
        elseif ffake == false then
            game:GetService('RunService'):UnbindFromRenderStep("fake")
            setfflag("S2PhysicsSenderRate",15)
            if _G.FAKELAG then
                _G.FAKELAG:Disconnect()
            end
        end
    end
    )
    trun:Toggle(
        "Auto-reload",
        false,
        function(reload)
            if reload == true then
                game:GetService('RunService'):BindToRenderStep("Auto-Reload", 0 , function()
                    if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                        if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo") then
                            if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("Ammo").Value <= 0 then
                                game:GetService("ReplicatedStorage").MainEvent:FireServer("Reload", game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")) 
                                wait(1)
                            end
                        end
                    end
                end)
            elseif reload == false then
                game:GetService('RunService'):UnbindFromRenderStep("Auto-Reload")
            end
        end
    )

    trun:Toggle(
        "anti lock เเบบ อยู่นิ่งๆ",
        false,
        function(locked)
            if locked == true then
                    _G.ANTILOCKNORANDOM = game:GetService("RunService").Heartbeat:Connect(function()
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid").WalkSpeed = 24
                        local currentvel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(68756,100,-344)
                        game:GetService("RunService").RenderStepped:Wait()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = currentvel
                        end)
            elseif locked == false then
                if _G.ANTILOCKNORANDOM then
                    _G.ANTILOCKNORANDOM:Disconnect()
                end
            end
        end
    )

    trun:Toggle(
        "anti lock เเบบ ขยับตัวไวๆ",
        false,
        function(lock)
            if lock == true then
                if _G.cons then
                    for _, v in pairs(_G.cons) do
                        v:Disconnect()
                    end
                
                    _G.cons = nil
                end
                
                local rsrv = game:GetService("RunService")
                local heartbeat = rsrv.Heartbeat
                local renderstepped = rsrv.RenderStepped
                
                local lp = game.Players.LocalPlayer
                local mouse = lp:GetMouse()
                
                local chr = lp.Character
                local hum = chr.Humanoid
                local root = hum.RootPart
                
                local isinvisible = false
                
                local visible_parts = {}
                
                for _, v in pairs(chr:GetDescendants()) do
                    if v:IsA("BasePart") and v.Transparency == 0 then
                        visible_parts[#visible_parts + 1] = v
                    end
                end
                
                local function ghost_parts()
                    for _, v in pairs(visible_parts) do
                        v.Transparency = v.Transparency == 0 and 0.5 or 0
                    end
                end
                
                local kdownv2 = mouse.KeyDown:Connect(function(key)
                    if key == "g" then
                        isinvisible = not isinvisible
                        ghost_parts()
                    end
                end)
                
                local loopv3 = heartbeat:Connect(function()
                    if isinvisible then
                        local oldcf = root.CFrame
                        local oldcamoffset = hum.CameraOffset
                
                        local newcf = oldcf * CFrame.new(math.random(-20,20), math.random(-20,30), math.random(-20,20))
                
                        hum.CameraOffset = newcf:ToObjectSpace(CFrame.new(oldcf.Position)).Position
                        root.CFrame = newcf
                
                        renderstepped:Wait()
                
                        hum.CameraOffset = oldcamoffset
                        root.CFrame = oldcf
                    end
                end)
                isinvisible = true
                _G.cons = {kdownv2, loopv3}
            else
                if _G.cons then
                    for _, v in pairs(_G.cons) do
                        v:Disconnect()
                    end
                
                    _G.cons = nil
                end
            end
        end
    )

    trun:Toggle(
        "โปร หายตัว",
        false,
        function(invisible)
            if invisible == true then
                
                local rsrv = game:GetService("RunService")
                local heartbeat = rsrv.Heartbeat
                local renderstepped = rsrv.RenderStepped
                
                local lp = game.Players.LocalPlayer
                local mouse = lp:GetMouse()
                
                local chr = lp.Character
                local hum = chr.Humanoid
                local root = hum.RootPart
                
                local isinvisible = false
                
                local visible_parts = {}
                
                for _, v in pairs(chr:GetDescendants()) do
                    if v:IsA("BasePart") and v.Transparency == 0 then
                        visible_parts[#visible_parts + 1] = v
                    end
                end
                
                local function ghost_parts()
                    for _, v in pairs(visible_parts) do
                        v.Transparency = v.Transparency == 0 and 0.5 or 0
                    end
                end
                
                local kdown = mouse.KeyDown:Connect(function(key)
                    if key == "g" then
                        print("sigma")
                    end
                end)
                
                local loop = heartbeat:Connect(function()
                    if isinvisible then
                        local oldcf = root.CFrame
                        local oldcamoffset = hum.CameraOffset
                        local newcf = oldcf * CFrame.new(math.random(-1000,1000),-500,0)
                        hum.CameraOffset = newcf:ToObjectSpace(CFrame.new(oldcf.Position)).Position
                        if game.Players.LocalPlayer.Character.BodyEffects.Movement:FindFirstChild("ReduceWalk") then
                            root.CFrame = oldcf * CFrame.new(0,1,0)
                        else
                        root.CFrame = newcf
                        end
                        renderstepped:Wait()
                
                        hum.CameraOffset = oldcamoffset
                        root.CFrame = oldcf
                    end
                end)
                
                _G.cons = {kdown, loop}
                isinvisible = true
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Trun on invis"; 

                    Duration = 60;
                    })
                    wait(0.1)
            elseif invisible == false then
                if _G.cons then
                    for _, v in pairs(_G.cons) do
                        v:Disconnect()
                    end
                    _G.cons = nil
                end
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Trun off invis"; 

                    Duration = 60;
                    })
            end
        end
    )

    trun:Toggle("block auto", false, function(t)
        if t == true then
            if getgenv().AUTO_BLOCK__ then
            else
            game.ReplicatedStorage.MainEvent:FireServer("ResetNew")game.Players.LocalPlayer.Character.Humanoid:ChangeState'Dead'
            end
            game:GetService("ReplicatedStorage").ClientAnimations.Block.AnimationId = "rbxassetid://0"
            local OWNER = game:GetService("Players").LocalPlayer
            local GUN_CHECK = nil
            task.wait()
            function Block()
                game.ReplicatedStorage.MainEvent:FireServer("Block",true) wait()
                game.ReplicatedStorage.MainEvent:FireServer("Block",false)
            end
            if getgenv().AUTO_BLOCK__ then getgenv().AUTO_BLOCK__:Disconnect() end
            getgenv().AUTO_BLOCK__ = game:GetService("RunService").Stepped:Connect(function()
                if blocking == true then
                    if OWNER.Character.BodyEffects:FindFirstChild('Block') then OWNER.Character.BodyEffects.Block:Destroy() end
                    GUN_CHECK = OWNER.Character:FindFirstChildWhichIsA("Tool")
                    if GUN_CHECK then
                        if GUN_CHECK:FindFirstChild("GunScript") then
                            game.ReplicatedStorage.MainEvent:FireServer("Block",false)
                        else
                            Block()
                        end
                    else
                        Block()
                    end
                    end
                end)
            blocking = true
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,150,0)
            task.wait()    
        elseif t == false then
            blocking = false
        end
    end)

