function boommmm()
    if OWNER.Backpack:FindFirstChild("Boom") then OWNER.Backpack:FindFirstChild("Boom"):Destroy() end

    local KIckAnim = Instance.new('Animation');
    KIckAnim.AnimationId = "rbxassetid://3354696735";--3354696735-or-8254765806
        tool = Instance.new("Tool")
        tool.RequiresHandle = false
        tool.Name = "Boom"
        tool.Activated:Connect(function()
            local plr = game.Players.LocalPlayer
            local lastpos = plr.Character.HumanoidRootPart.CFrame
            
            for i = 1,200 do 
                plr.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop["[Grenade] - $743"].Head.CFrame
                fireclickdetector(game.Workspace.Ignored.Shop["[Grenade] - $743"].ClickDetector)
                wait()
            end
            plr.Character.HumanoidRootPart.CFrame = lastpos
            
            for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                wait(0.05)
                spawn(function()
                    if v.Name == "[Grenade]" then
                        v.Parent = plr.Character; v:Activate(); wait(0.1); v:Deactivate(); wait(0.1); v:Activate()
                    end
                end)
            end
            plr.Character.HumanoidRootPart.CFrame = lastpos
            wait(1)
            for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                wait(0.05)
                spawn(function()
                    if v.Name == "[Grenade]" then
                        v.Parent = plr.Character; v:Activate(); wait(0.1); v:Deactivate(); wait(0.1); v:Activate()
                    end
                end)
            end
        end)
        tool.Parent = game.Players.LocalPlayer.Backpack
    end;
    game:GetService('Players').LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR');
    boommmm()
    function kissck()
        if OWNER.Backpack:FindFirstChild("Hold") then OWNER.Backpack:FindFirstChild("Hold"):Destroy() end
        local KIckAnim = Instance.new('Animation');
        KIckAnim.AnimationId = "rbxassetid://3355740058";
            tool = Instance.new("Tool")
            tool.RequiresHandle = false
            tool.Name = "Hold"
            tool.Activated:Connect(function()
                game.ReplicatedStorage.MainEvent:FireServer("Grabbing",true)
                task.wait(0.1)
                if game.Players.LocalPlayer.Character.BodyEffects.Grabbed.Value ~= nil then
                Play(2848703459)
                end
            end)
            tool.Parent = game.Players.LocalPlayer.Backpack
        end;
        game:GetService('Players').LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR');
        kissck()
    function kick()
        if OWNER.Backpack:FindFirstChild("DIO Teleport") then OWNER.Backpack:FindFirstChild("DIO Teleport"):Destroy() end

        local KIckAnim = Instance.new('Animation');
        KIckAnim.AnimationId = "rbxassetid://3355740058";
            tool = Instance.new("Tool")
            tool.RequiresHandle = false
            tool.Name = "DIO Teleport"
            tool.Activated:Connect(function()
                Play(6603980575)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -30)
                TweenService:Create(workspace.CurrentCamera, TweenInfo.new(0.5), {FieldOfView = 85}):Play()
                TweenService:Create(game:GetService("Lighting").ColorCorrection, TweenInfo.new(0.8), {Saturation = -1}):Play()
                local blur = Instance.new("BlurEffect", game:GetService("Lighting"))
                TweenService:Create(blur, TweenInfo.new(0.8), {Size = 10}):Play()
                wait(0.5)
                TweenService:Create(workspace.CurrentCamera, TweenInfo.new(0.5), {FieldOfView = 70}):Play()
                game:GetService("Lighting").ColorCorrection.TintColor = Color3.fromRGB(250, 250, 250)
                TweenService:Create(blur, TweenInfo.new(0.8), {Size = 0}):Play()
                wait(0.3)
                TweenService:Create(game:GetService("Lighting").ColorCorrection, TweenInfo.new(0.8), {Saturation = 0}):Play()
            end)
            tool.Parent = game.Players.LocalPlayer.Backpack
        end;
        game:GetService('Players').LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR');
        kick()


        function did()
            if OWNER.Backpack:FindFirstChild("I'm gonna rip you in half now!") then OWNER.Backpack:FindFirstChild("I'm gonna rip you in half now!"):Destroy() end
            local KIckAnim = Instance.new('Animation');
            KIckAnim.AnimationId = "rbxassetid://3096047107";
                tool = Instance.new("Tool")
                tool.RequiresHandle = false
                tool.Name = "I'm gonna rip you in half now!"
                tool.Activated:Connect(function()
                    Play(7148332723)
                    wait(2.5)
                    Play(5951833277)--5951833277
                    wait(.1)
                    wait(.1)
                    for i , v in pairs(game.Players[tostring(game.Players.LocalPlayer.Character.BodyEffects.Grabbed.Value)].Character:GetChildren()) do
                        if v:IsA("MeshPart") then
                            v.Parent.LowerTorso.Position = Vector3.new(0,-1200,0)
                        end
                    end
                    wait(.1)
                    game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
                end)
                tool.Parent = game.Players.LocalPlayer.Backpack
        
            end;
        game:GetService('Players').LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR');
        did()

