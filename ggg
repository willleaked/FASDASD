                v:Stop()
            end
        elseif msg:lower() == "!unfreeze." then
            PP.Character.HumanoidRootPart.Anchored = false
        elseif msg:lower() == "!reset." then
            game.ReplicatedStorage.MainEvent:FireServer("ResetNew")game.Players.LocalPlayer.Character.Humanoid:ChangeState'Dead'
        elseif msg:lower() == "/e ok" then
            chat("HELLO")
        elseif msg:lower() == "/e test" then
            chat("HELLO")
        elseif msg:lower() == "!rejoin." then
            game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
        elseif msg:lower() == "!dance." then
            PP.Character.HumanoidRootPart.CFrame = game.Players[MainAAccount].Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-2)
            local DefaultDanceanim = Instance.new('Animation');
            DefaultDanceanim.AnimationId = "rbxassetid://11710529975";
            game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(DefaultDanceanim):Play()
        end
        end)
    end) 
    end)
end

end



function chat(msg)
    local A_1 = msg
    local A_2 = "All"
    local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
    Event:FireServer(A_1, A_2)
end
local delay = 0.1 -- Adjust this value to control the speed
local timer = 0
local waitTime = 30
local waiting = false
if _G.Chatloop then _G.Chatloop:Disconnect() end
_G.Chatloop = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
timer = timer + deltaTime

-- Check if we are not waiting
if not waiting then
    if timer >= delay then
        if game.Players:FindFirstChild("poohvvff") then
            chat("ไอตุ๊ด "..game.Players.poohvvff.DisplayName)
            chat("จอก")
            print("Running code")
            timer = 0 -- Reset the timer
            
            -- Set waiting to true and start the wait
            waiting = true
            coroutine.wrap(function()
                wait(waitTime)
                waiting = false -- Reset waiting after waitTime seconds
            end)()
        end
    end
end
end)
