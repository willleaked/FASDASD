                        Anim:Stop()
                    end
                    
                    Character.Animate.Disabled = false
                    
                    AT0, AT1, AP, A0 = Align(Ins.Parent.LowerTorso, LocalPlayer.Character.UpperTorso, Vector3.new(1, 3, -4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.UpperTorso, LocalPlayer.Character.UpperTorso, Vector3.new(1, 5, -4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.RightUpperArm, LocalPlayer.Character.UpperTorso, Vector3.new(-4, 3, -4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.LeftUpperArm, LocalPlayer.Character.UpperTorso, Vector3.new(6, 3, -4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.RightUpperLeg, LocalPlayer.Character.UpperTorso, Vector3.new(-4, 0, -4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.LeftUpperLeg, LocalPlayer.Character.UpperTorso, Vector3.new(6, 0, -4), Vector3.new(180, 0, 180))
    
                    AT0, AT1, AP, A0 = Align(Ins.Parent.RightLowerArm, LocalPlayer.Character.LowerTorso, Vector3.new(-4, 3, 4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.LeftLowerArm, LocalPlayer.Character.LowerTorso, Vector3.new(6, 3, 4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.RightLowerLeg, LocalPlayer.Character.LowerTorso, Vector3.new(-4, 0, 4), Vector3.new(180, 0, 180))
                    AT0, AT1, AP, A0 = Align(Ins.Parent.LeftLowerLeg, LocalPlayer.Character.LowerTorso, Vector3.new(6, 0, 4), Vector3.new(180, 0, 180))
    
                    wait(.1)
                    local DefaultDanceanim = Instance.new('Animation');
                    DefaultDanceanim.AnimationId = "rbxassetid://15549124879";
                    local gyat = game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(DefaultDanceanim)
                    gyat:Play()
                    gyat:AdjustSpeed(0.2)
                    Play(9057725705)
                    wait(.1)
                    Play(9057725705)
                    wait(6)
                    wait(1)
    
                    Ins.Parent.RightUpperLeg.Position = Vector3.new(0,-1200,0)
                    wait(1)
                    Ins.Parent.LeftUpperLeg.Position = Vector3.new(0,-1200,0)
                    wait(1)
                    Ins.Parent.LowerTorso.Position = Vector3.new(0,-1200,0)
                    wait(1)
                    Ins.Parent.RightUpperArm.Position = Vector3.new(0,-1200,0)
                    wait(1)
                    Ins.Parent.LeftUpperArm.Position = Vector3.new(0,-1200,0)
                    wait(1)
                    game.ReplicatedStorage.MainEvent:FireServer("Grabbing")
    
    
        
                end
        
                repeat task.wait() until Ins.Parent == nil
                
                Character.Animate.Disabled = true
                    
            for _, Anim in pairs(Character.Humanoid:GetPlayingAnimationTracks()) do
                Anim:Stop()
            end
            
            Character.Animate.Disabled = false
                
                AT0:Destroy()
                AT1:Destroy()
                AP:Destroy()
                A0:Destroy()
            end
        end)
    end)
