                    _1:Stop()
                    _2:Stop()
                    _3:Stop()
                    _4:Play()
                    _4:AdjustSpeed(2.4)
                elseif (v.Animation.AnimationId:match("rbxassetid://2788289281")) then
                    v:Stop(0);
                elseif (v.Animation.AnimationId:match("rbxassetid://2788292075")) then
                    v:Stop(0);
                    _2:Stop()
                    _3:Stop()
                    _1:Stop()
                    _5:Play()
