        local PP = game.Players.LocalPlayer
        game:GetService("RunService").RenderStepped:Connect(function()
            pcall(function()
            -- Chat Connect --
            local MainAccount = "zer1ckq" -- MainAccount Username
            game.Players[MainAccount].Chatted:Connect(function(msg)
            if msg:lower() == "!cash." then
                local args = {
                    [1] = "DropMoney",
                    [2] = "10000"
                }
                
                game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer(unpack(args))        
            elseif msg:lower() == "!bring." then
                PP.Character.HumanoidRootPart.CFrame = game.Players[MainAccount].Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-2)
            elseif msg:lower() == "!log." then
                game:Shutdown()
            elseif msg:lower() == "!freeze." then
                PP.Character.HumanoidRootPart.Anchored = true
            elseif msg:lower() == "!sleep." then
                
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,0)* CFrame.Angles(math.pi*0.5,0,0)
                game.Players.LocalPlayer.Character.Humanoid.Sit = true
                task.wait(0.1)
                local Char = game.Players.LocalPlayer.Character
                local Hum = Char:FindFirstChildOfClass("Humanoid") or Char:FindFirstChildOfClass("AnimationController")
            
                for i,v in next, Hum:GetPlayingAnimationTracks() do
                    v:Stop()
                end
            elseif msg:lower() == "!unfreeze." then
                PP.Character.HumanoidRootPart.Anchored = false
            elseif msg:lower() == "!reset." then
                game.ReplicatedStorage.MainEvent:FireServer("ResetNew")game.Players.LocalPlayer.Character.Humanoid:ChangeState'Dead'
            elseif msg:lower() == "/e ok" then
                chat("HELLO")
            elseif msg:lower() == "/e test" then
                chat("HELLO")
            elseif msg:lower() == "!rejoin." then
                game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
            elseif msg:lower() == "!dance." then
                PP.Character.HumanoidRootPart.CFrame = game.Players[MainAccount].Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-2)
                local DefaultDanceanim = Instance.new('Animation');
                DefaultDanceanim.AnimationId = "rbxassetid://11710529975";
                game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(DefaultDanceanim):Play()
            end
            end)
        end) 
        end)
    end


