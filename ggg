    local tools = serv:Channel("ของปั่นๆ tools")
    tools:Button("auto Frie armor",function()
        function armorloop()
            while wait() do
                if game.Players.LocalPlayer.Character:WaitForChild('BodyEffects')['FireArmor'].Value <= 0 then
                        local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
                                 local Brokie = game.Workspace.Ignored.Shop["[Fire Armor] - $2493"]
                                 game.Players.LocalPlayer.Character:MoveTo(Brokie.Head.Position)
                                 wait(0.1)
                                 fireclickdetector(Brokie.ClickDetector)
                                 wait(0.1)
                          game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(savepos)
                end
                end
                end
                
                game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                armorloop()
                
                game.Players.LocalPlayer.CharacterAdded:connect(function()
                    game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                    wait(0.5)
                    armorloop()
                end)
            end)
            tools:Button("auto armor",function()
        function armorloop()
            while wait() do
                if game.Players.LocalPlayer.Character:WaitForChild('BodyEffects')['Armor'].Value <= 30 then
                        local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
                                 local Brokie = game.Workspace.Ignored.Shop["[High-Medium Armor] - $2440"]
                                 game.Players.LocalPlayer.Character:MoveTo(Brokie.Head.Position)
                                 wait(0.1)
                                 fireclickdetector(Brokie.ClickDetector)
                                 wait(0.1)
                          game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(savepos)
                end
                end
                end
                
                game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                armorloop()
                
                game.Players.LocalPlayer.CharacterAdded:connect(function()
                    game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                    wait(0.5)
                    armorloop()
                end)
            end)
    tools:Button("auto eat",function()
        function Anti()
            while wait() do
            if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').Health <= 50 then
                    local savepos = game.Players.LocalPlayer.Character.UpperTorso.Position
                             local Brokie = game.Workspace.Ignored.Shop["[Taco] - $2"]
                             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(584, 40, -474)
                             wait(0.25)
                             fireclickdetector(Brokie.ClickDetector)
                             wait(0.25)
                         wait(.25)
                     local surg = game.Players.LocalPlayer.Backpack:FindFirstChild("[Taco]")
                      surg.Parent = game.Players.LocalPlayer.Character
                      local New = game.Players.LocalPlayer.Character:FindFirstChild("[Taco]")
                      for i = 1,55  do      
                      New:Activate()
                      wait(0.1)
                      New:Activate()
                      end
                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(savepos)
            end
            end
            end
            
            game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                Anti()
            
            game.Players.LocalPlayer.CharacterAdded:connect(function()
                game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                wait(0.5)
                Anti()
            end)
    end)
tools:Button("ถ้าเลือดน้อยจะวาปเร็ว",function()

    game:GetService("RunService").stepped:Connect(function()
        if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').Health <= 50 then
            spinmo = true
        else
            spinmo = false
        end
    end)
    
    
    game:GetService("RunService").Heartbeat:Connect(function()
        if spinmo == true then
        local currentvel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(math.random(-2000,2000), math.random(-2000,2000), math.random(-2000,2000))
        game:GetService("RunService").RenderStepped:Wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = currentvel
        end
    end)
end)
    tools:Button(
        "real kick",
        function()
        
            getgenv().asfafgagdrfgerth = 1000
            getgenv().wsgewgeherhhrsd = 390
        
            game:GetService("RunService").heartbeat:Connect(function()
                for _, v in next, game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks() do
                    if (v.Animation.AnimationId:match("rbxassetid://2788307715")) then
                        local Vel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector.X*getgenv().asfafgagdrfgerth,getgenv().asfafgagdrfgerth,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector.Z*getgenv().asfafgagdrfgerth)
                        game:GetService("RunService").RenderStepped:Wait()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vel
                    end
                end
            end)
        
            
            function kick()
                local KIckAnim = Instance.new('Animation');
                KIckAnim.AnimationId = "rbxassetid://2788307715";
                    tool = Instance.new("Tool")
                    tool.RequiresHandle = false
                    tool.Name = "pro kick"
                    tool.Activated:Connect(function()
                        game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(KIckAnim):Play()
                    wait(0.1)
                    Play(5696182212)
                    wait(0.1)
            
                    end)
                    tool.Parent = game.Players.LocalPlayer.Backpack
                end;
                game:GetService('Players').LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR');
                kick()
        end)
